{
  "name": "tcc-api",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "dev": "nodemon src/main.ts",
    "start": "node dist/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc",
    "typeorm": "TS_NODE_PROJECT=tsconfig.json TS_NODE_TRANSPILE_ONLY=true ts-node node_modules/typeorm/cli -f src/ormconfig.ts",
    "db:update": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:generate-migration": "npm run typeorm -- migration:generate -n"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.0",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "4.7.9",
    "@nestjs/typeorm": "7.1.0",
    "@types/node": "16",
    "config-decorators": "0.2.0",
    "csvtojson": "2.0.10",
    "install": "^0.13.0",
    "npm": "^8.19.2",
    "pg": "8.5.1",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.2.32",
    "uuid": "8.1.0"
  }
}
